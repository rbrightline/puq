{
  "name": "@puq/gen",
  "license": "MIT",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "Generator",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "keywords": [
    "puq",
    "gen"
  ],
  "repository": {
    "directory": "libs/gen",
    "url": "https://github.com/rbrightline/puq/tree/main",
    "type": "github"
  },
  "homepage": "https://rbrightline.github.io/puq/gen",
  "bugs": {
    "email": "robert.brightline@gmail.com",
    "url": "https://rbrightline.github.io"
  },
  "categories": [
    "Other"
  ],
  "contributors": [
    {
      "email": "robert.brightline@gmail.com",
      "name": "Robert Brightline",
      "url": "https://rbrightline.github.io"
    }
  ],
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "icon": "https://raw.githubusercontent.com/rbrightline/puq/refs/heads/main/libs/gen/favicon.png",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "generators.json",
    "!**/*.tsbuildinfo",
    "generators.json"
  ],
  "nx": {
    "sourceRoot": "libs/gen/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/gen/dist",
          "main": "libs/gen/src/index.ts",
          "tsConfig": "libs/gen/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "doc": {
        "command": "cd libs/gen && npx compodoc -d ../../public/gen  -p tsconfig.lib.json -n gen"
      }
    }
  },
  "devDependencies": {
    "@swc/helpers": "~0.5.11",
    "@types/js-yaml": "^4.0.9"
  },
  "generators": "./generators.json",
  "dependencies": {
    "@nx/devkit": "20.4.4",
    "@puq/debug": "^0.0.1",
    "@puq/fs": "^0.0.8",
    "@puq/printer": "^0.0.2",
    "@puq/type": "^0.0.27",
    "js-yaml": "^4.1.0"
  }
}