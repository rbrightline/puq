{
    "name": "@puq/entity",
    "license": "MIT",
    "version": "0.4.11",
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "description": "Database entities, views, and dtos generated by @puq/gen:entity <directory>/<model-name>",
    "author": {
        "email": "robert.brightline@gmail.com",
        "name": "Robert Brightline",
        "url": "https://rbrightline.github.io"
    },
    "keywords": [
        "puq",
        "entity"
    ],
    "repository": {
        "directory": "libs/entity",
        "url": "https://github.com/rbrightline/puq/tree/main",
        "type": "github"
    },
    "homepage": "https://rbrightline.github.io/puq/entity",
    "bugs": {
        "email": "robert.brightline@gmail.com",
        "url": "https://rbrightline.github.io"
    },
    "categories": [
        "Other"
    ],
    "contributors": [
        {
            "email": "robert.brightline@gmail.com",
            "name": "Robert Brightline",
            "url": "https://rbrightline.github.io"
        }
    ],
    "funding": [
        "https://cash.app/$puqlib"
    ],
    "icon": "https://raw.githubusercontent.com/rbrightline/puq/refs/heads/main/libs/entity/favicon.png",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "!**/*.tsbuildinfo"
    ],
    "nx": {
        "sourceRoot": "libs/entity/src",
        "targets": {
            "build": {
                "executor": "@nx/js:swc",
                "outputs": [
                    "{options.outputPath}"
                ],
                "options": {
                    "outputPath": "libs/entity/dist",
                    "main": "libs/entity/src/index.ts",
                    "tsConfig": "libs/entity/tsconfig.lib.json",
                    "skipTypeCheck": false,
                    "stripLeadingPaths": true
                },
                "dependsOn": [
                    "lint"
                ]
            },
            "doc": {
                "command": "cd libs/entity && npx compodoc -d ../../public/entity  -p tsconfig.lib.json -n entity"
            }
        }
    },
    "devDependencies": {
        "@faker-js/faker": "^9.5.1",
        "@puq/model": "workspace:^",
        "@puq/type": "workspace:^",
        "better-sqlite3": "^11.8.1",
        "typeorm": "^0.3.20"
    },
    "peerDependencies": {
        "@puq/property": "^0.4.11",
        "@puq/orm": "^0.4.11"
    },
    "dependencies": {
        "@swc/helpers": "^0.5.15"
    }
}