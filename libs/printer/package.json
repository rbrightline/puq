{
  "name": "@puq/printer",
  "license": "MIT",
  "version": "0.3.3",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "A utility library that transforms property options such as `{ type:'string'}` into various property definitions like plain (`name: string`), entity (`@Column() name:string`), dto (`@Property() name: string`), or view (`@ViewColumn() name: string`) property definitions for TypeScript/JavaScript projects.",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "keywords": [
    "puq",
    "printer"
  ],
  "repository": {
    "directory": "libs/printer",
    "url": "https://github.com/rbrightline/puq/tree/main",
    "type": "github"
  },
  "homepage": "https://rbrightline.github.io/puq/printer",
  "bugs": {
    "email": "robert.brightline@gmail.com",
    "url": "https://rbrightline.github.io"
  },
  "categories": [
    "Other"
  ],
  "contributors": [
    {
      "email": "robert.brightline@gmail.com",
      "name": "Robert Brightline",
      "url": "https://rbrightline.github.io"
    }
  ],
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "icon": "https://raw.githubusercontent.com/rbrightline/puq/refs/heads/main/libs/printer/favicon.png",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "libs/printer/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/printer/dist",
          "main": "libs/printer/src/index.ts",
          "tsConfig": "libs/printer/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        },
        "dependsOn": [
          "lint"
        ]
      },
      "doc": {
        "command": "cd libs/printer && npx compodoc -d ../../public/printer  -p tsconfig.lib.json -n printer"
      }
    }
  },
  "devDependencies": {
    "@puq/type": "^0.3.3"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.15",
    "@puq/is": "^0.3.3"
  }
}
