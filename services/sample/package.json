{
  "name": "@puq/sample",
  "license": "MIT",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "description": "Sample service",
  "author": {
    "email": "robert.brightline@gmail.com",
    "name": "Robert Brightline",
    "url": "https://rbrightline.github.io"
  },
  "keywords": [
    "puq",
    "sample"
  ],
  "repository": {
    "directory": "services/sample",
    "url": "https://github.com/rbrightline/puq/tree/main",
    "type": "github"
  },
  "homepage": "https://rbrightline.github.io/puq/sample",
  "bugs": {
    "email": "robert.brightline@gmail.com",
    "url": "https://rbrightline.github.io"
  },
  "categories": [
    "Other"
  ],
  "contributors": [
    {
      "email": "robert.brightline@gmail.com",
      "name": "Robert Brightline",
      "url": "https://rbrightline.github.io"
    }
  ],
  "funding": [
    "https://cash.app/$puqlib"
  ],
  "icon": "https://raw.githubusercontent.com/rbrightline/puq/refs/heads/main/services/sample/favicon.png",
  "bin": {
    "sample": "./bin/sample-service.js"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "bin",
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "services/sample/src",
    "targets": {
      "build": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "services/sample/dist",
          "main": "services/sample/src/index.ts",
          "tsConfig": "services/sample/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true
        }
      },
      "build:watch": {
        "executor": "@nx/js:swc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "services/sample/dist",
          "main": "services/sample/src/index.ts",
          "tsConfig": "services/sample/tsconfig.lib.json",
          "skipTypeCheck": false,
          "stripLeadingPaths": true,
          "watch": true
        }
      },
      "start": {
        "command": "cd services/sample &&  node --watch --watch-path=src ./bin/sample-service.js sample"
      },
      "serve": {
        "dependsOn": [
          "build:watch",
          "start"
        ]
      },
      "doc": {
        "command": "cd services/sample && npx compodoc -d ../../public/sample  -p tsconfig.lib.json -n sample"
      }
    }
  },
  "dependencies": {
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@puq/core": "^0.0.6",
    "better-sqlite3": "^11.8.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^13.1.0",
    "helmet": "^8.0.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@swc/helpers": "~0.5.11"
  }
}
